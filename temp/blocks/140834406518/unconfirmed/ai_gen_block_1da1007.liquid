{% doc %}
  @prompt
    Create a high-converting landing page for "Endarkened Narratives Lab" with a sophisticated, academic aesthetic matching Sienna & Slate's brand. Include: 1) Hero section with compelling headline about transforming academic narratives and research journeys, 2) Problem/solution section addressing academic burnout and research challenges, 3) Features section highlighting introspective methodologies and community support, 4) Social proof area for testimonials, 5) Clear pricing/membership tiers with benefits, 6) FAQ section, 7) Strong call-to-action buttons throughout. Use warm, earthy color palette with gold accents, elegant typography, and plenty of white space. Optimize for mobile responsiveness and fast loading.

{% enddoc %}
{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% style %}
  .en-landing-{{ ai_gen_id }} {
    font-family: {{ block.settings.text_font }};
    color: {{ block.settings.text_color }};
    background-color: {{ block.settings.background_color }};
  }

  .en-landing-{{ ai_gen_id }} h1,
  .en-landing-{{ ai_gen_id }} h2,
  .en-landing-{{ ai_gen_id }} h3,
  .en-landing-{{ ai_gen_id }} h4 {
    font-family: {{ block.settings.heading_font }};
    color: {{ block.settings.heading_color }};
    margin-top: 0;
    line-height: 1.3;
  }

  .en-landing-{{ ai_gen_id }} h1 {
    font-size: 2.8rem;
    margin-bottom: 1.5rem;
  }

  .en-landing-{{ ai_gen_id }} h2 {
    font-size: 2.2rem;
    margin-bottom: 1.2rem;
  }

  .en-landing-{{ ai_gen_id }} h3 {
    font-size: 1.8rem;
    margin-bottom: 1rem;
  }

  .en-landing-{{ ai_gen_id }} h4 {
    font-size: 1.4rem;
    margin-bottom: 0.8rem;
  }

  .en-landing-{{ ai_gen_id }} p {
    line-height: 1.6;
    margin-bottom: 1.5rem;
  }

  .en-section-{{ ai_gen_id }} {
    padding: 5rem 0;
  }

  .en-container-{{ ai_gen_id }} {
    max-width: 1200px;
    margin: 0 auto;
    padding: 0 2rem;
  }

  .en-btn-{{ ai_gen_id }} {
    display: inline-block;
    padding: 0.8rem 2rem;
    background-color: {{ block.settings.primary_button_color }};
    color: {{ block.settings.primary_button_text_color }};
    text-decoration: none;
    border-radius: 4px;
    font-weight: 600;
    transition: all 0.3s ease;
    border: none;
    cursor: pointer;
    text-align: center;
  }

  .en-btn-{{ ai_gen_id }}:hover {
    background-color: {{ block.settings.primary_button_hover_color }};
  }

  .en-btn-secondary-{{ ai_gen_id }} {
    background-color: {{ block.settings.secondary_button_color }};
    color: {{ block.settings.secondary_button_text_color }};
  }

  .en-btn-secondary-{{ ai_gen_id }}:hover {
    background-color: {{ block.settings.secondary_button_hover_color }};
  }

  /* Hero Section */
  .en-hero-{{ ai_gen_id }} {
    text-align: center;
    padding: 8rem 0;
    background-color: {{ block.settings.hero_background_color }};
    position: relative;
    overflow: hidden;
  }

  .en-hero-content-{{ ai_gen_id }} {
    max-width: 800px;
    margin: 0 auto;
    position: relative;
    z-index: 2;
  }

  .en-hero-{{ ai_gen_id }} h1 {
    margin-bottom: 1.5rem;
  }

  .en-hero-{{ ai_gen_id }} p {
    font-size: 1.2rem;
    margin-bottom: 2rem;
  }

  .en-hero-cta-{{ ai_gen_id }} {
    display: flex;
    justify-content: center;
    gap: 1rem;
    margin-top: 2rem;
  }

  /* Problem/Solution Section */
  .en-problem-solution-{{ ai_gen_id }} {
    background-color: {{ block.settings.problem_solution_background_color }};
  }

  .en-problem-solution-grid-{{ ai_gen_id }} {
    display: grid;
    grid-template-columns: 1fr 1fr;
    gap: 3rem;
  }

  .en-problem-{{ ai_gen_id }}, 
  .en-solution-{{ ai_gen_id }} {
    padding: 2rem;
    border-radius: 8px;
  }

  .en-problem-{{ ai_gen_id }} {
    background-color: {{ block.settings.problem_background_color }};
  }

  .en-solution-{{ ai_gen_id }} {
    background-color: {{ block.settings.solution_background_color }};
  }

  /* Features Section */
  .en-features-{{ ai_gen_id }} {
    text-align: center;
    background-color: {{ block.settings.features_background_color }};
  }

  .en-features-grid-{{ ai_gen_id }} {
    display: grid;
    grid-template-columns: repeat(3, 1fr);
    gap: 2rem;
    margin-top: 3rem;
  }

  .en-feature-{{ ai_gen_id }} {
    padding: 2rem;
    border-radius: 8px;
    background-color: {{ block.settings.feature_background_color }};
    transition: transform 0.3s ease;
  }

  .en-feature-{{ ai_gen_id }}:hover {
    transform: translateY(-10px);
  }

  .en-feature-icon-{{ ai_gen_id }} {
    font-size: 2.5rem;
    margin-bottom: 1rem;
    color: {{ block.settings.accent_color }};
  }

  /* Testimonials Section */
  .en-testimonials-{{ ai_gen_id }} {
    text-align: center;
    background-color: {{ block.settings.testimonials_background_color }};
  }

  .en-testimonials-grid-{{ ai_gen_id }} {
    display: grid;
    grid-template-columns: repeat(3, 1fr);
    gap: 2rem;
    margin-top: 3rem;
  }

  .en-testimonial-{{ ai_gen_id }} {
    padding: 2rem;
    border-radius: 8px;
    background-color: {{ block.settings.testimonial_background_color }};
    text-align: left;
  }

  .en-testimonial-content-{{ ai_gen_id }} {
    font-style: italic;
    margin-bottom: 1.5rem;
  }

  .en-testimonial-author-{{ ai_gen_id }} {
    display: flex;
    align-items: center;
  }

  .en-testimonial-author-image-{{ ai_gen_id }} {
    width: 50px;
    height: 50px;
    border-radius: 50%;
    margin-right: 1rem;
    overflow: hidden;
  }

  .en-testimonial-author-image-{{ ai_gen_id }} img {
    width: 100%;
    height: 100%;
    object-fit: cover;
  }

  .en-testimonial-author-image-{{ ai_gen_id }} svg {
    width: 100%;
    height: 100%;
    background-color: #f4f4f4;
  }

  .en-testimonial-author-info-{{ ai_gen_id }} h4 {
    margin: 0;
    font-size: 1.1rem;
  }

  .en-testimonial-author-info-{{ ai_gen_id }} p {
    margin: 0;
    font-size: 0.9rem;
    color: {{ block.settings.text_muted_color }};
  }

  /* Pricing Section */
  .en-pricing-{{ ai_gen_id }} {
    text-align: center;
    background-color: {{ block.settings.pricing_background_color }};
  }

  .en-pricing-grid-{{ ai_gen_id }} {
    display: grid;
    grid-template-columns: repeat(3, 1fr);
    gap: 2rem;
    margin-top: 3rem;
  }

  .en-pricing-plan-{{ ai_gen_id }} {
    padding: 2.5rem 2rem;
    border-radius: 8px;
    background-color: {{ block.settings.plan_background_color }};
    text-align: left;
    transition: transform 0.3s ease;
    border: 1px solid {{ block.settings.border_color }};
  }

  .en-pricing-plan-{{ ai_gen_id }}:hover {
    transform: translateY(-10px);
  }

  .en-pricing-plan-{{ ai_gen_id }}.featured {
    border: 2px solid {{ block.settings.accent_color }};
    position: relative;
  }

  .en-pricing-plan-{{ ai_gen_id }}.featured::before {
    content: "Most Popular";
    position: absolute;
    top: -12px;
    left: 50%;
    transform: translateX(-50%);
    background-color: {{ block.settings.accent_color }};
    color: white;
    padding: 0.3rem 1rem;
    border-radius: 20px;
    font-size: 0.8rem;
    font-weight: 600;
  }

  .en-plan-header-{{ ai_gen_id }} {
    margin-bottom: 2rem;
    text-align: center;
  }

  .en-plan-price-{{ ai_gen_id }} {
    font-size: 2.5rem;
    font-weight: 700;
    margin-bottom: 0.5rem;
    color: {{ block.settings.heading_color }};
  }

  .en-plan-billing-{{ ai_gen_id }} {
    font-size: 0.9rem;
    color: {{ block.settings.text_muted_color }};
  }

  .en-plan-features-{{ ai_gen_id }} {
    margin: 2rem 0;
  }

  .en-plan-feature-{{ ai_gen_id }} {
    display: flex;
    align-items: flex-start;
    margin-bottom: 1rem;
  }

  .en-plan-feature-icon-{{ ai_gen_id }} {
    margin-right: 0.8rem;
    color: {{ block.settings.accent_color }};
    flex-shrink: 0;
    margin-top: 0.3rem;
  }

  .en-plan-cta-{{ ai_gen_id }} {
    margin-top: 2rem;
    text-align: center;
  }

  /* FAQ Section */
  .en-faq-{{ ai_gen_id }} {
    background-color: {{ block.settings.faq_background_color }};
  }

  .en-faq-grid-{{ ai_gen_id }} {
    display: grid;
    grid-template-columns: 1fr 1fr;
    gap: 2rem;
    margin-top: 3rem;
  }

  .en-faq-item-{{ ai_gen_id }} {
    margin-bottom: 1.5rem;
  }

  .en-faq-question-{{ ai_gen_id }} {
    font-weight: 600;
    margin-bottom: 0.8rem;
    color: {{ block.settings.heading_color }};
    font-size: 1.2rem;
  }

  .en-faq-answer-{{ ai_gen_id }} {
    color: {{ block.settings.text_color }};
  }

  /* CTA Section */
  .en-cta-{{ ai_gen_id }} {
    text-align: center;
    background-color: {{ block.settings.cta_background_color }};
    padding: 6rem 0;
  }

  .en-cta-content-{{ ai_gen_id }} {
    max-width: 700px;
    margin: 0 auto;
  }

  .en-cta-buttons-{{ ai_gen_id }} {
    display: flex;
    justify-content: center;
    gap: 1rem;
    margin-top: 2rem;
  }

  /* Responsive Styles */
  @media (max-width: 992px) {
    .en-features-grid-{{ ai_gen_id }},
    .en-testimonials-grid-{{ ai_gen_id }},
    .en-pricing-grid-{{ ai_gen_id }} {
      grid-template-columns: repeat(2, 1fr);
    }
  }

  @media (max-width: 768px) {
    .en-problem-solution-grid-{{ ai_gen_id }},
    .en-faq-grid-{{ ai_gen_id }} {
      grid-template-columns: 1fr;
    }

    .en-hero-{{ ai_gen_id }} {
      padding: 5rem 0;
    }

    .en-hero-{{ ai_gen_id }} h1 {
      font-size: 2.2rem;
    }

    .en-section-{{ ai_gen_id }} {
      padding: 3rem 0;
    }
  }

  @media (max-width: 576px) {
    .en-features-grid-{{ ai_gen_id }},
    .en-testimonials-grid-{{ ai_gen_id }},
    .en-pricing-grid-{{ ai_gen_id }} {
      grid-template-columns: 1fr;
    }

    .en-hero-cta-{{ ai_gen_id }},
    .en-cta-buttons-{{ ai_gen_id }} {
      flex-direction: column;
      gap: 1rem;
    }

    .en-btn-{{ ai_gen_id }} {
      width: 100%;
    }

    .en-hero-{{ ai_gen_id }} h1 {
      font-size: 1.8rem;
    }

    .en-hero-{{ ai_gen_id }} p {
      font-size: 1rem;
    }
  }

  /* Accent Elements */
  .en-accent-line-{{ ai_gen_id }} {
    width: 80px;
    height: 4px;
    background-color: {{ block.settings.accent_color }};
    margin: 0 auto 2rem;
  }

  .en-section-heading-{{ ai_gen_id }} {
    text-align: center;
    margin-bottom: 3rem;
  }
{% endstyle %}

<div class="en-landing-{{ ai_gen_id }}" {{ block.shopify_attributes }}>
  <!-- Hero Section -->
  <section class="en-hero-{{ ai_gen_id }} en-section-{{ ai_gen_id }}">
    <div class="en-container-{{ ai_gen_id }}">
      <div class="en-hero-content-{{ ai_gen_id }}">
        <h1>{{ block.settings.hero_heading }}</h1>
        <p>{{ block.settings.hero_subheading }}</p>
        <div class="en-hero-cta-{{ ai_gen_id }}">
          <a href="{{ block.settings.primary_button_link }}" class="en-btn-{{ ai_gen_id }}">{{ block.settings.primary_button_text }}</a>
          <a href="{{ block.settings.secondary_button_link }}" class="en-btn-{{ ai_gen_id }} en-btn-secondary-{{ ai_gen_id }}">{{ block.settings.secondary_button_text }}</a>
        </div>
      </div>
    </div>
  </section>

  <!-- Problem/Solution Section -->
  <section class="en-problem-solution-{{ ai_gen_id }} en-section-{{ ai_gen_id }}">
    <div class="en-container-{{ ai_gen_id }}">
      <div class="en-section-heading-{{ ai_gen_id }}">
        <div class="en-accent-line-{{ ai_gen_id }}"></div>
        <h2>{{ block.settings.problem_solution_heading }}</h2>
        <p>{{ block.settings.problem_solution_subheading }}</p>
      </div>
      <div class="en-problem-solution-grid-{{ ai_gen_id }}">
        <div class="en-problem-{{ ai_gen_id }}">
          <h3>{{ block.settings.problem_heading }}</h3>
          <p>{{ block.settings.problem_description }}</p>
        </div>
        <div class="en-solution-{{ ai_gen_id }}">
          <h3>{{ block.settings.solution_heading }}</h3>
          <p>{{ block.settings.solution_description }}</p>
        </div>
      </div>
    </div>
  </section>

  <!-- Features Section -->
  <section class="en-features-{{ ai_gen_id }} en-section-{{ ai_gen_id }}">
    <div class="en-container-{{ ai_gen_id }}">
      <div class="en-section-heading-{{ ai_gen_id }}">
        <div class="en-accent-line-{{ ai_gen_id }}"></div>
        <h2>{{ block.settings.features_heading }}</h2>
        <p>{{ block.settings.features_subheading }}</p>
      </div>
      <div class="en-features-grid-{{ ai_gen_id }}">
        <div class="en-feature-{{ ai_gen_id }}">
          <div class="en-feature-icon-{{ ai_gen_id }}">
            <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><path d="M2 3h6a4 4 0 0 1 4 4v14a3 3 0 0 0-3-3H2z"></path><path d="M22 3h-6a4 4 0 0 0-4 4v14a3 3 0 0 1 3-3h7z"></path></svg>
          </div>
          <h3>{{ block.settings.feature1_heading }}</h3>
          <p>{{ block.settings.feature1_description }}</p>
        </div>
        <div class="en-feature-{{ ai_gen_id }}">
          <div class="en-feature-icon-{{ ai_gen_id }}">
            <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><circle cx="12" cy="12" r="10"></circle><path d="M8 14s1.5 2 4 2 4-2 4-2"></path><line x1="9" y1="9" x2="9.01" y2="9"></line><line x1="15" y1="9" x2="15.01" y2="9"></line></svg>
          </div>
          <h3>{{ block.settings.feature2_heading }}</h3>
          <p>{{ block.settings.feature2_description }}</p>
        </div>
        <div class="en-feature-{{ ai_gen_id }}">
          <div class="en-feature-icon-{{ ai_gen_id }}">
            <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><path d="M17 21v-2a4 4 0 0 0-4-4H5a4 4 0 0 0-4 4v2"></path><circle cx="9" cy="7" r="4"></circle><path d="M23 21v-2a4 4 0 0 0-3-3.87"></path><path d="M16 3.13a4 4 0 0 1 0 7.75"></path></svg>
          </div>
          <h3>{{ block.settings.feature3_heading }}</h3>
          <p>{{ block.settings.feature3_description }}</p>
        </div>
      </div>
    </div>
  </section>

  <!-- Testimonials Section -->
  <section class="en-testimonials-{{ ai_gen_id }} en-section-{{ ai_gen_id }}">
    <div class="en-container-{{ ai_gen_id }}">
      <div class="en-section-heading-{{ ai_gen_id }}">
        <div class="en-accent-line-{{ ai_gen_id }}"></div>
        <h2>{{ block.settings.testimonials_heading }}</h2>
        <p>{{ block.settings.testimonials_subheading }}</p>
      </div>
      <div class="en-testimonials-grid-{{ ai_gen_id }}">
        <div class="en-testimonial-{{ ai_gen_id }}">
          <div class="en-testimonial-content-{{ ai_gen_id }}">
            <p>{{ block.settings.testimonial1_content }}</p>
          </div>
          <div class="en-testimonial-author-{{ ai_gen_id }}">
            <div class="en-testimonial-author-image-{{ ai_gen_id }}">
              {% if block.settings.testimonial1_image %}
                <img src="{{ block.settings.testimonial1_image | image_url: width: 100 }}" alt="{{ block.settings.testimonial1_name }}">
              {% else %}
                {{ 'image' | placeholder_svg_tag }}
              {% endif %}
            </div>
            <div class="en-testimonial-author-info-{{ ai_gen_id }}">
              <h4>{{ block.settings.testimonial1_name }}</h4>
              <p>{{ block.settings.testimonial1_title }}</p>
            </div>
          </div>
        </div>
        <div class="en-testimonial-{{ ai_gen_id }}">
          <div class="en-testimonial-content-{{ ai_gen_id }}">
            <p>{{ block.settings.testimonial2_content }}</p>
          </div>
          <div class="en-testimonial-author-{{ ai_gen_id }}">
            <div class="en-testimonial-author-image-{{ ai_gen_id }}">
              {% if block.settings.testimonial2_image %}
                <img src="{{ block.settings.testimonial2_image | image_url: width: 100 }}" alt="{{ block.settings.testimonial2_name }}">
              {% else %}
                {{ 'image' | placeholder_svg_tag }}
              {% endif %}
            </div>
            <div class="en-testimonial-author-info-{{ ai_gen_id }}">
              <h4>{{ block.settings.testimonial2_name }}</h4>
              <p>{{ block.settings.testimonial2_title }}</p>
            </div>
          </div>
        </div>
        <div class="en-testimonial-{{ ai_gen_id }}">
          <div class="en-testimonial-content-{{ ai_gen_id }}">
            <p>{{ block.settings.testimonial3_content }}</p>
          </div>
          <div class="en-testimonial-author-{{ ai_gen_id }}">
            <div class="en-testimonial-author-image-{{ ai_gen_id }}">
              {% if block.settings.testimonial3_image %}
                <img src="{{ block.settings.testimonial3_image | image_url: width: 100 }}" alt="{{ block.settings.testimonial3_name }}">
              {% else %}
                {{ 'image' | placeholder_svg_tag }}
              {% endif %}
            </div>
            <div class="en-testimonial-author-info-{{ ai_gen_id }}">
              <h4>{{ block.settings.testimonial3_name }}</h4>
              <p>{{ block.settings.testimonial3_title }}</p>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>

  <!-- Pricing Section -->
  <section class="en-pricing-{{ ai_gen_id }} en-section-{{ ai_gen_id }}">
    <div class="en-container-{{ ai_gen_id }}">
      <div class="en-section-heading-{{ ai_gen_id }}">
        <div class="en-accent-line-{{ ai_gen_id }}"></div>
        <h2>{{ block.settings.pricing_heading }}</h2>
        <p>{{ block.settings.pricing_subheading }}</p>
      </div>
      <div class="en-pricing-grid-{{ ai_gen_id }}">
        <div class="en-pricing-plan-{{ ai_gen_id }}">
          <div class="en-plan-header-{{ ai_gen_id }}">
            <h3>{{ block.settings.plan1_name }}</h3>
            <div class="en-plan-price-{{ ai_gen_id }}">{{ block.settings.plan1_price }}</div>
            <div class="en-plan-billing-{{ ai_gen_id }}">{{ block.settings.plan1_billing }}</div>
          </div>
          <div class="en-plan-features-{{ ai_gen_id }}">
            {% assign features = block.settings.plan1_features | newline_to_br | split: '<br />' %}
            {% for feature in features %}
              {% if feature != blank %}
                <div class="en-plan-feature-{{ ai_gen_id }}">
                  <div class="en-plan-feature-icon-{{ ai_gen_id }}">
                    <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><polyline points="20 6 9 17 4 12"></polyline></svg>
                  </div>
                  <div>{{ feature }}</div>
                </div>
              {% endif %}
            {% endfor %}
          </div>
          <div class="en-plan-cta-{{ ai_gen_id }}">
            <a href="{{ block.settings.plan1_button_link }}" class="en-btn-{{ ai_gen_id }}">{{ block.settings.plan1_button_text }}</a>
          </div>
        </div>
        <div class="en-pricing-plan-{{ ai_gen_id }} featured">
          <div class="en-plan-header-{{ ai_gen_id }}">
            <h3>{{ block.settings.plan2_name }}</h3>
            <div class="en-plan-price-{{ ai_gen_id }}">{{ block.settings.plan2_price }}</div>
            <div class="en-plan-billing-{{ ai_gen_id }}">{{ block.settings.plan2_billing }}</div>
